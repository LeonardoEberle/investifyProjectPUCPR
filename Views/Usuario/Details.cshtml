@model Investify.Models.UsuarioModel

@{
    ViewData["Title"] = "Perfil";
}

<h1>Perfil</h1>

<div>
    <h4>Usuario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nome
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usu_nome)
        </dd>
        <dt class = "col-sm-2">
            CPF
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usu_cpf)
        </dd>
        <dt class = "col-sm-2">
            Telefone
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usu_telefone)
        </dd>
        <dt class = "col-sm-2">
            E-Mail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usu_email)
        </dd>
        <dt class = "col-sm-2">
            Data de Inscricao
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usu_dataInicio)
        </dd>
        <dt class = "col-sm-2">
            Data de Nascimento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usu_dataNasc)
        </dd>
        <dt class = "col-sm-2">
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Usu_id">Edit</a> |
    <a asp-action="Index">Lista de Usuario</a>
</div>
